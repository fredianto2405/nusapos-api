							"listen": "test",
							"script": {
								"exec": [
									"let response = pm.response.json();\r",
									"if (response && response.data && response.data.access_token) {\r",
									"    pm.environment.set(\"accessToken\", response.data.access_token);\r",
									"    console.log(\"access token saved to environment:\", response.data.access_token);\r",
									"} else {\r",
									"    console.error(\"access token not found in response!\");\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
